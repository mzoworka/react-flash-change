{
    "name": "@avinlab/react-flash-change",
    "version": "1.2.0",
    "description": "Flash props change",
    "author": "Avin Grape <carcinogen75@gmail.com> (https://github.com/avin/)",
    "contributors": [
        "Avin Grape <carcinogen75@gmail.com> (https://github.com/avin/)"
    ],
    "license": "MIT",
    "homepage": "https://github.com/avin/react-flash-change",
    "repository": {
        "type": "git",
        "url": "https://github.com/avin/react-flash-change"
    },
    "bugs": {
        "url": "https://github.com/avin/react-flash-change/issues"
    },
    "engines": {
        "node": ">8.0.0"
    },
    "keywords": [
        "react",
        "reactjs",
        "change",
        "flash",
        "backlight"
    ],
    "main": "dist/index.cjs.js",
    "module": "dist/index.esm.js",
    "files": [
        "dist",
        "src/*.js",
        "README.md"
    ],
    "scripts": {
        "build": "rimraf dist && mkdir dist && yarn build:flow && yarn build:source",
        "build:demo": "build-storybook -o demo",
        "build:flow": "cpr flow-template dist/index.cjs.js.flow && cpr flow-template dist/index.esm.js.flow",
        "build:source": "rollup -c",
        "flow": "flow check --max-warnings=0 src && flow check website",
        "linc": "lint-staged",
        "lint": "eslint '**/*.js'",
        "precommit": "lint-staged",
        "prepare": "yarn run build",
        "prettier": "prettier --write \"**/*.{js,json,css}\"",
        "start": "rollup -c -w",
        "storybook": "start-storybook -p 9009",
        "test": "cross-env CI=1 react-scripts test --env=jsdom",
        "test:watch": "react-scripts test --env=jsdom"
    },
    "lint-staged": {
        "linters": {
            "**/*.{js,json,css,md}": [
                "prettier --write",
                "git add"
            ],
            "**/*.js": "eslint --max-warnings 0"
        },
        "concurrent": false
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "dependencies": {
        "@babel/runtime": "^7.17.8",
        "clsx": "^1.1.1",
        "react-lifecycles-compat": "^3.0.4"
    },
    "peerDependencies": {
        "react": ">= 15.0.0",
        "react-dom": ">= 15.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.8",
        "@babel/plugin-proposal-class-properties": "^7.16.7",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.16.11",
        "@babel/preset-flow": "^7.16.7",
        "@babel/preset-react": "^7.16.7",
        "@storybook/addon-actions": "^6.4.20",
        "@storybook/addon-links": "^6.4.20",
        "@storybook/addons": "^6.4.20",
        "@storybook/react": "^6.4.20",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^9.0.0",
        "babel-loader": "^8.2.4",
        "babel-plugin-annotate-pure-calls": "^0.4.0",
        "cpr": "^3.0.1",
        "cross-env": "^7.0.3",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.6",
        "eslint": "^8.12.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-config-react-app": "^7.0.0",
        "eslint-config-standard": "^16.0.3",
        "eslint-config-standard-react": "^11.0.1",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jsx-a11y": "^6",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-standard": "^4.1.0",
        "flow-bin": "^0.175.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.7",
        "prettier": "^2.6.2",
        "react": "^18.0.0",
        "react-dom": "^18.0.0",
        "react-scripts": "^5.0.0",
        "react-test-renderer": "^18.0.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.70.1",
        "rollup-plugin-babel": "^4.0.2",
        "rollup-plugin-commonjs": "^8.2.1",
        "rollup-plugin-node-resolve": "^3.0.2"
    }
}
